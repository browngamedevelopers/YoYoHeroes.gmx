          <argument>
            <kind>1</kind>
            <string>

if (!sticking){
    imagestart = 0;
    imageend = 1;
}

var below = position_meeting(x,y + sprite_height/2 + 8,objAllObjects);
var left = position_meeting(bbox_left,y + sprite_height/2 + 8,objAllObjects);
var right = position_meeting(bbox_right,y + sprite_height/2 + 8,objAllObjects);

var canmove = false;
var movingdown = false;

if (below &amp;&amp; (left || right)) {
    canmove = true;
}

if(phy_speed_y &gt; 0){
    //falling
    imagestart = 7;
    imageend = 10;
    movingdown = true;
} else if (phy_speed_y &lt; 0){
    //jumping
    imagestart = 1;
    imageend = 1;
    wasmovingup = true;
    movingdown = false;
} else {
    if(movingdown) {
        canmove = true;
        movingdown = false;
    }
}

if(phy_speed_y &gt; MAX_FALL_SPEED){
    phy_speed_y = MAX_FALL_SPEED;
}

if(keyboard_check(vk_left) || keyboard_check(ord('A'))){
    image_xscale = -1;
    if(canmove){
        imagestart = 1;
        imageend = 7;
        phy_speed_x  = -PLAYER_SPEED;
    }
}
if(keyboard_check(vk_right) || keyboard_check(ord('D'))){
    image_xscale = 1;
    if(canmove){
        imagestart = 1;
        imageend = 7;
        phy_speed_x  = PLAYER_SPEED;
    }
}else if (canmove) {
    //phy_speed_x = 0;
}
if(keyboard_check_pressed(vk_up) || keyboard_check_pressed(ord('W'))){
    if(canmove){
        phy_speed_y = -1 * PLAYER_JUMP;
    }
}
physics_apply_force(phy_com_x, phy_com_y, 0, ROOM_GRAVITY*phy_mass);

if(canmove){
    //on ground
    //show_debug_message("ground");
    if(abs(phy_speed_x) &lt; 1){
       phy_speed_x = 0;
    } else {
       //phy_speed_x -= sign(phy_speed_x)*0.7;
       phy_speed_x *= .9;
    }
}else {
    //in air
    //show_debug_message("air");
    friction = 0;
}


if (x &gt; global.farthest){
    score = x;
    global.farthest = x;
}
if(phy_speed_x &gt; 1){
    image_xscale = 1;
}
if(phy_speed_x &lt; -1){
    image_xscale = -1;
}
if(mouse_check_button(mb_left)){
    imagestart = 14;
    imageend = 15;
    shootYoyo(mouse_x, mouse_y);
}
phy_rotation = 0;
if (sticking) {
    var yoyo = instance_find(objYoyo, 0);
    var playerYoyoDistance = distance_to_point(yoyo.x, yoyo.y)
    if (playerYoyoDistance &lt; EPSILON_YOYO_PLAYER) {
        imagestart = 18;
        imageend = 22;
    }else{
        phy_rotation = 90 -point_direction(x,y,yoyo.x,yoyo.y);
        show_debug_message(-1*point_direction(x,y,yoyo.x,yoyo.y));
        imagestart = 10;
        imageend = 13;
    }
}
image_index = max(imagestart, image_index mod imageend);

</string>
